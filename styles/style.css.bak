    transform: translateY(-50%);
    max-width: 970px;
    width: 100%;
    margin: 0 auto;
}

.faq-block {
    padding: 17px 25px 36px 69px;
}

.wrapper-dark {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.8);
    z-index: 10000;
    display: none;
    transition: 0.3s;
}
.container-faq {
    display: none;
    transition: 0.3s;
}

.active-faq {
    display: block;
    transition: 0.3s;
}

.title-window {
